/**
 * 16-bit Not:
 * for i=0..15: out[i] = not in[i]
 */

CHIP Not16 {
    IN in[16];
    OUT out[16];

    PARTS:
    Not8 ( a = a[0..7], b = b[0..7], out = out[0..7]);
    Not8 ( a = a[8..15], b = b[8..15], out = out[8..15]);
}
